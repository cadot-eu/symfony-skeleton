{
  "name": "cadot.eu/symfony-skeleton-light",
  "type": "template",
  "license": "MIT",
  "description": "A skeleton light for symfony 5",
  "keywords": [
    "symfony5",
    "skeleton",
    "webpack",
    "encore",
    "faker",
    "phpunit",
    "liip",
    "doctrine",
    "patches",
    "pdf",
    "sentry",
    "stripe"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "cadot.eu/twigbundle": "^4.0",
    "composer/package-versions-deprecated": "1.11.99.2",
    "cweagans/composer-patches": "^1.7",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.9",
    "fakerphp/faker": "^1.16",
    "gedmo/doctrine-extensions": "^3.1",
    "gregwar/captcha": "^1.1",
    "gumlet/php-image-resize": "^2.0",
    "h4cc/wkhtmltopdf-amd64": "^0.12.4",
    "knplabs/doctrine-behaviors": "^2.2",
    "liip/imagine-bundle": "^2.6",
    "myclabs/deep-copy": "^1.10",
    "nyholm/psr7": "^1.4",
    "phpdocumentor/reflection-docblock": "^5.2",
    "presta/sitemap-bundle": "^3.1",
    "sensio/framework-extra-bundle": "^5.1",
    "sentry/sentry-symfony": "^4.2",
    "spatie/schema-org": "^3.10",
    "stof/doctrine-extensions-bundle": "^1.6",
    "stripe/stripe-php": "^7.100",
    "symfony/asset": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "5.4.*",
    "symfony/framework-bundle": "5.4.*",
    "symfony/http-client": "5.4.*",
    "symfony/intl": "5.4.*",
    "symfony/mailer": "5.4.*",
    "symfony/messenger": "5.4.*",
    "symfony/mime": "5.4.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/notifier": "5.4.*",
    "symfony/panther": "^1.1",
    "symfony/process": "5.4.*",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/string": "5.4.*",
    "symfony/translation": "5.4.*",
    "symfony/twig-bundle": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/web-link": "5.4.*",
    "symfony/webpack-encore-bundle": "^1.12",
    "symfony/yaml": "5.4.*",
    "symplify/vendor-patches": "^9.3",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/string-extra": "^3.3",
    "twig/twig": "^2.12|^3.0",
    "wgenial/php-mimetypeicon": "^1.7",
    "woodsandwalker/faker-picture": "^1.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/debug-bundle": "5.4.*",
    "symfony/maker-bundle": "^1.34",
    "symfony/phpunit-bridge": "^5.4",
    "symfony/stopwatch": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*",
    "zenstruck/browser": "0.9.1",
    "zenstruck/foundry": "^1.16"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true",
      "require": "5.4.*"
    }
  }
}